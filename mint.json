{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Wool Ball",
  "logo": {
    "light": "/logo/logo.png",
    "dark": "/logo/logo.png"
  },
  "favicon": "/favicon.svg",
  "colors": {
    "primary": "#0D9373",
    "light": "#07C983",
    "dark": "#0D9373",
    "anchors": {
      "from": "#0D9373",
      "to": "#07C983"
    }
  },
  "openapi": "/api-reference/openapi.json",
  "api": {
    "baseUrl": "https://api.woolball.xyz",
    "auth": {
      "method": "bearer"
    }
  },
  "topbarLinks": [
    {
      "name": "Support",
      "url": "mailto:eduardo.worrel@gmail.com"
    }
  ],
  "topbarCtaButton": {
    "name": "Dashboard",
    "url": "https://woolball.xyz/Identity/Account/Manage/"
  },
  "tabs": [
    {
      "name": "API Reference",
      "url": "api-reference"
    }
  ],
  "anchors": [
    {
      "name": "Community",
      "icon": "discord",
      "url": "https://discord.gg/xbSmMfmwWW"
    },
    {
      "name": "Browser as a Server",
      "icon": "network-wired",
      "url": "https://woolball.xyz/Early"
    },
    {
      "name": "Blog",
      "icon": "newspaper",
      "url": "https://woolball.xyz/#card-container"
    }
  ],
  "navigation": [
    {
      "group": "Get Started",
      "pages": [
        "introduction",
        "Browser"
      ]
    },
    {
      "group": "API Documentation",
      "pages": [
        "api-reference/introduction"
      ]
    },
    {
      "group": "Text Services",
      "pages": [
        "api-reference/endpoint/completions",
        "api-reference/endpoint/completions-models",
        "api-reference/endpoint/translation",
        "api-reference/endpoint/translation-languages",
        "api-reference/endpoint/zero-shot-classification",
        "api-reference/endpoint/summarization"
      ]
    },
    {
      "group": "Audio Services",
      "pages": [
        "api-reference/endpoint/text-to-speech",
        "api-reference/endpoint/speech-to-text",
        "api-reference/endpoint/speech-to-text-models"
      ]
    },
    {
      "group": "Image Services",
      "pages": [
        "api-reference/endpoint/vision",
        "api-reference/endpoint/image-classification",
        "api-reference/endpoint/image-facial-emotions",
        "api-reference/endpoint/image-zero-shot",
        "api-reference/endpoint/char-to-image"
      ]
    }
  ],
  "footerSocials": {
    "github": "https://github.com/woolball-xyz"
  },
  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true
  },
  "search": {
    "prompt": "Search Wool Ball API docs..."
  },
  "modeToggle": {
    "default": "dark"
  }
}
